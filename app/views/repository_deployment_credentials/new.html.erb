<%= labelled_tabular_form_for(:repository_deployment_credential,
          url: { controller: 'repository_deployment_credentials', action: 'create' },
          method: :post ) do |f| %>

  <%# if @gitolite_deploy_keys.nil? || @gitolite_deploy_keys.empty? %>
    <%#= link_to 'Add a new deployment key', {controller: 'my_public_keys', action: 'index'}, class: 'button -alt-highlight' %>
  <%# else %>
  <fieldset class="new_repository_deployment_credential">
    <legend><%= 'Create new deployment credential' %></legend>
      <%= render :partial => 'repository_deployment_credentials/form', :locals => { :f => f} %>
      <%= styled_button_tag l(:button_create), class: '-highlight -with-icon icon-yes' %>
    <%# end %>
  </fieldset>
<% end %>
      